#include <iostream>
#include<bits/stdc++.h>
#include<climits>
using namespace std;

int main() {
	int n;
	cin>>n;
	int p;
	cin>>p;
	int sum=1;
	if(p%2==0){
	  for(int i=1;i<=p/2;i++){
	     sum*=n; 
	  }
	  cout<<sum*sum<<endl;
	}
	else{
	   for(int i=1;i<=p/2;i++){
	     sum*=n; 
	  }
	  cout<<sum*n*sum<<endl; 
	}
	return 0;
}
